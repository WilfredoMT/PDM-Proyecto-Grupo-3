<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoPDM"
        tools:targetApi="31">
        <activity
            android:name=".ui.solicitudes.EditarSolicitudesHorarioActivity"
            android:exported="false" />
        <activity
            android:name=".ui.solicitudes.AgregarSolicitudesHorarioActivity"
            android:exported="false" />
        <activity
            android:name=".ui.asignar.EditarAsignacionActivity"
            android:exported="false" />
        <activity
            android:name=".ui.asignar.AgregarAsignacionActivity"
            android:exported="false" />
        <activity
            android:name=".ui.encargados.EditarEncargadosActivity"
            android:exported="false" />
        <activity
            android:name=".ui.encargados.AgregarEncargadosActivity"
            android:exported="false" />
        <activity
            android:name=".ui.coordinadores.EditarCoordinadoresActivity"
            android:exported="false" />
        <activity
            android:name=".ui.coordinadores.AgregarCoordinadoresActivity"
            android:exported="false" />
        <activity
            android:name=".ui.locales.EditarLocalesActivity"
            android:exported="false" />
